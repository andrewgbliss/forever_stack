{
    "name": "forever-stack/php-api",
    "description": "PHP API for Forever Stack",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "slim/slim": "^4.11",
        "slim/psr7": "^1.6",
        "php-di/php-di": "^7.0",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
